{
  "name": "on-tour-app",
  "private": true,
  "version": "2.2.2",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm:conditional && SKIP_WASM=true VITE_SKIP_WASM=true vite build",
    "build:production": "NODE_ENV=production npm run build:wasm:conditional && SKIP_WASM=true VITE_SKIP_WASM=true vite build --mode production",
    "build:wasm": "cd wasm-financial-engine && wasm-pack build --target web --out-dir pkg --release",
    "build:wasm:safe": "npm run build:wasm || echo 'WASM build skipped - using JavaScript fallback'",
    "build:wasm:conditional": "if [ \"$VERCEL\" != \"1\" ]; then npm run build:wasm:safe; else echo 'Vercel build - skipping WASM compilation'; fi",
    "build:check": "tsc --noEmit || true && npm run build",
    "build:strict": "tsc && npm run build",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "build:perf": "npm run build && node scripts/analyze-performance.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test:run",
    "migrate:subcollections": "tsx scripts/migrateToSubCollections.ts",
    "clean": "rm -rf dist node_modules/.vite",
    "optimize": "npm run clean && npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "migrate:prophecy": "node scripts/migrate-prophecy-to-firestore.mjs",
    "migrate:contacts": "node scripts/migrate-contacts-only.mjs"
  },
  "dependencies": {
