{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci --legacy-peer-deps --prefer-offline",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "NPM_CONFIG_LOGLEVEL": "error",
      "VITE_BUILD_CACHE": "true"
    }
  },
  "crons": []
}
